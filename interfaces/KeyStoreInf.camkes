procedure KeyStoreInf {
    include "SeosError.h";
    include "SeosKeyStoreRpc.h";

    seos_err_t      getRpcHandle    (inout SeosKeyStoreRpc_Handle handle);
    void            closeRpcHandle  (in SeosKeyStoreRpc_Handle handle);
};

procedure KeyStoreRpcInf {
    include "SeosError.h";
    include "SeosKeyStoreRpc.h";

    seos_err_t      importKey       (in SeosKeyStoreRpc_Handle handle, in size_t keySize);
