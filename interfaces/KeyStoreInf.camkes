    seos_err_t      deleteKey       (in SeosKeyStoreRpc_Handle handle);
    seos_err_t      copyKey         (in SeosKeyStoreRpc_Handle handle, in SeosKeyStoreRpc_Handle destKeyStore);
    seos_err_t      moveKey         (in SeosKeyStoreRpc_Handle handle, in SeosKeyStoreRpc_Handle destKeyStore);
    seos_err_t      wipeKeyStore    (in SeosKeyStoreRpc_Handle handle);
